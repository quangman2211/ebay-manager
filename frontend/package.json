{
  "name": "ebay-manager-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.16",
    "@mui/lab": "^5.0.0-alpha.170",
    "@mui/material": "^5.14.17",
    "@mui/x-data-grid": "^6.20.4",
    "@mui/x-date-pickers": "^8.11.0",
    "axios": "^1.6.2",
    "dayjs": "^1.11.18",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-router-dom": "^6.20.1",
    "react-scripts": "5.0.1",
    "recharts": "^3.1.2",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:unit": "react-scripts test --watchAll=false --coverage",
    "test:unit:watch": "react-scripts test --coverage",
    "test:integration": "react-scripts test --testPathPattern=integration --watchAll=false",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:coverage": "react-scripts test --coverage --watchAll=false --coverageReporters=text lcov html",
    "test:coverage-report": "react-scripts test --coverage --watchAll=false --coverageReporters=html",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:analyze": "node scripts/coverage-analysis.js",
    "playwright:install": "playwright install",
    "playwright:codegen": "playwright codegen localhost:3000",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^16.18.68",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "babel-jest": "^30.1.2",
    "identity-obj-proxy": "^3.0.0",
    "msw": "^1.3.5",
    "playwright": "^1.55.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
